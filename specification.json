{
	"$schema": "https://raw.githubusercontent.com/hashicorp/terraform-plugin-codegen-spec/main/spec/v0.1/schema.json",
	"version": "0.1",
	"provider": {
		"name": "sentry",
		"schema": {
			"attributes": [
				{
					"name": "token",
					"string": {
						"description": "The authentication token used to connect to Sentry. The value can be sourced from the `SENTRY_AUTH_TOKEN` environment variable.",
						"optional_required": "optional",
						"sensitive": true
					}
				},
				{
					"name": "base_url",
					"string": {
						"description": "The target Sentry Base API URL in the format `https://[hostname]/api/`. The default value is `https://sentry.io/api/`. The value must be provided when working with Sentry On-Premise. The value can be sourced from the `SENTRY_BASE_URL` environment variable.",
						"optional_required": "optional"
					}
				}
			]
		}
	},
	"resources": [],
	"datasources": [
		{
			"name": "organization",
			"schema": {
				"markdown_description": "Sentry Organization data source.",
				"attributes": [
					{
						"name": "slug",
						"string": {
							"description": "The unique URL slug for the organization.",
							"computed_optional_required": "required"
						}
					},
					{
						"name": "internal_id",
						"string": {
							"description": "The internal ID for this organization.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "name",
						"string": {
							"description": "The human readable name for this organization.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "id",
						"string": {
							"description": "The unique URL slug for this organization. **Deprecated** Use `slug` instead.",
							"deprecation_message": "Use `slug` instead.",
							"computed_optional_required": "computed"
						}
					}
				]
			}
		},
		{
			"name": "team",
			"schema": {
				"markdown_description": "Sentry Team data source.",
				"attributes": [
					{
						"name": "organization",
						"string": {
							"description": "The unique URL slug or internal ID of the organization.",
							"computed_optional_required": "required"
						}
					},
					{
						"name": "slug",
						"string": {
							"description": "The unique URL slug for the team.",
							"computed_optional_required": "required"
						}
					},
					{
						"name": "internal_id",
						"string": {
							"description": "The internal ID for this team.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "name",
						"string": {
							"description": "The human readable name for this team.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "id",
						"string": {
							"description": "The unique URL slug for this team. **Deprecated** Use `slug` instead.",
							"deprecation_message": "Use `slug` instead.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "has_access",
						"bool": {
							"deprecation_message": "This field is deprecated and will be removed in a future version.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "is_pending",
						"bool": {
							"deprecation_message": "This field is deprecated and will be removed in a future version.",
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "is_member",
						"bool": {
							"deprecation_message": "This field is deprecated and will be removed in a future version.",
							"computed_optional_required": "computed"
						}
					}
				]
			}
		}
	]
}